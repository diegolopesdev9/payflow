modules = ["nodejs-20", "web", "postgresql-16"]

[nix]
channel = "stable-25_05"

run = ["npm", "run", "dev"]

[deployment]
deploymentTarget = "autoscale"
run = ["npm", "run", "start"]
build = ["npm", "run", "build"]

[[ports]]
localPort = 5173
externalPort = 5173

[[ports]]
localPort = 8080
externalPort = 80

[agent]
expertMode = true
